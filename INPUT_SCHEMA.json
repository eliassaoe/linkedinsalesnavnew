{
    "title": "LinkedIn Profile Scraper",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "linkedinCookies": {
            "title": "LinkedIn Cookies",
            "type": "array",
            "description": "Your LinkedIn cookies",
            "editor": "json"
        },
        "profileUrl": {
            "title": "Profile URL",
            "type": "string",
            "description": "LinkedIn profile URL to scrape",
            "editor": "textfield",
            "example": "https://www.linkedin.com/in/eliasse-hamour-08194821a/"
        },
        "maxPages": {
            "title": "Max Pages",
            "type": "integer", 
            "description": "Maximum pages to scrape",
            "default": 50,
            "editor": "number"
        }
    },
    "required": ["linkedinCookies", "profileUrl"]
}
